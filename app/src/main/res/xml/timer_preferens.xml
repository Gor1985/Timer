<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
<CheckBoxPreference
android:defaultValue="true"
    android:key="enable sound"
    android:title="Enable Sound"
    android:summaryOff="Sound Disable"
    android:summaryOn="Sound Enable"
    />
  <ListPreference
      android:title="TimerMelody"
      android:key="TitleMelody"
      android:defaultValue="alarm"
      android:entries="@array/labelsMelodi"
      android:entryValues="@array/MelodyValues"

      />
  <EditTextPreference
      android:title="Defolt interval"
      android:key="defolt interval"
      android:defaultValue="30"
      />

</PreferenceScreen>